{
    "headers":[
        {
            "source":"/(.*)",
            "headers":[
                {
                    "key":"Access-Control-Allow-Origin",
                    "value":"*"
                },
                {
                    "key":"Access-Control-Allow-Credentials",
                    "value":"true"
                },
                {
                    "key":"Access-Control-Allow-Methods",
                    "value":"GET, POST, PUT, OPTIONS"
                }
            ]
        }
    ],
    "functions":{
        "api/message.js":{
            "maxDuration": 60
        },
        "api/auth.js":{
            "maxDuration":30
        }
    }
}